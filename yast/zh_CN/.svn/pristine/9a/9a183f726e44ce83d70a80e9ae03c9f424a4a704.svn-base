msgstr "未定义名称。"

#: src/modules/Wagon.ycp:634
#, ycp-format
msgid "Temporary migration product <b>%1</b> will be removed"
msgstr "临时迁移产品 <b>%1</b> 将删除"

#: src/modules/Wagon.ycp:642
#, ycp-format
msgid "<font color='red'><b>Warning:</b> Product <b>%1</b> will be removed.</font>"
msgstr "<font color='red'><b>警告：</b> 产品 <b>%1</b> 将删除。</font>"

#: src/modules/Wagon.ycp:650
#, ycp-format
msgid "<font color='red'><b>Error:</b> Product <b>%1</b> will be automatically removed.</font>"
msgstr "<font color='red'><b>错误：</b> 产品 <b>%1</b> 将自动删除。</font>"

#: src/modules/Wagon.ycp:680
#, ycp-format
msgid "Product <b>%1</b> will be upgraded"
msgstr "产品 <b>%1</b> 将升级"

#: src/modules/Wagon.ycp:687
#, ycp-format
msgid "Product <b>%1</b> will be upgraded to <b>%2</b>"
msgstr "产品 <b>%1</b> 将升级到 <b>%2</b>"

#: src/modules/Wagon.ycp:695
#, ycp-format
msgid "New product <b>%1</b> will be installed"
msgstr "新产品 <b>%1</b> 将安装"

#: src/modules/Wagon.ycp:711
#, ycp-format
msgid "Product <b>%1</b> will stay installed"
msgstr "产品 <b>%1</b> 将保留"

#: src/modules/Wagon.ycp:718
msgid "Download all packages before upgrade: "
msgstr "在升级前下载所有软件包："

#: src/modules/Wagon.ycp:719
msgid "Enabled"
msgstr "已启用"

#: src/modules/Wagon.ycp:719
msgid "Disabled"
msgstr "已禁用"

#. help text
#: src/modules/Wagon.ycp:726
msgid "<p>To change the update settings, go to <b>Packages Proposal</b> section.</p>"
msgstr "<p>要更改更新设置，请至<b>软件包方案</b>部分。</p>"

#: src/modules/Wagon.ycp:734
#, ycp-format
msgid ""
"<li><b>%1 products will be removed.\n"
"Go to the packages proposal and resolve the issue manually.<br>\n"
"It is safe to abort the migration now.</b></li>\n"
msgstr ""
