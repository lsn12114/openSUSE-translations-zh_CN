#. *
#. * Help text suffix for some types of the media
#.
#: src/modules/SourceDialogs.ycp:68
msgid ""
"<p>If the location is a file holding an ISO image\n"
"of the media, set <b>ISO Image</b>.</p>"
msgstr ""
"<p>如果该位置是保存介质的 ISO \n"
"映像的文件，请设置 <b>ISO 映像</b>。</p>"

#. *
#. * Help text suffix for some types of the media
#.
#: src/modules/SourceDialogs.ycp:74
msgid ""
"<p>If the repository is on multiple media,\n"
"set the location of the first media of the set.</p>\n"
msgstr ""
"<p>如果该软件仓库在多个介质上，\n"
"请设置该组的第一个介质的位置。</p>\n"

#. popup message
#: src/modules/SourceDialogs.ycp:306
msgid "The name of the repository cannot be empty."
msgstr "软件源名称不能为空。"

#. text entry
#: src/modules/SourceDialogs.ycp:320
msgid "&Repository Name"
msgstr "软件源名称(&R)"

#. help text
#: src/modules/SourceDialogs.ycp:329
msgid ""
"<p><big><b>Repository Name</b></big><br>\n"
"Use <b>Repository Name</b> to specify the name of the repository. If it is empty, YaST will use the product name (if available) or the URL as the name.</p>\n"
msgstr ""
"<p><big><b>软件源名称</b></big><br>\n"
"使用<b>软件源名称</b>指定软件源名称。如果名称为空，YaST 将会使用产品名(如果有的话)或者 URL 作为名称。</p>\n"

#. text entry
#: src/modules/SourceDialogs.ycp:342
msgid "&Service Name"
msgstr "服务名称(&S)"

#. help text
#: src/modules/SourceDialogs.ycp:346
msgid ""
"<p><big><b>Service Name</b></big><br>\n"
"Use <b>Service Name</b> to specify the name of the service. If it is empty, YaST will use part of the service URL as the name.</p>\n"
msgstr ""
"<p><big><b>服务名称</b></big><br>\n"
"使用<b>服务名称</b>指定服务名。如果名称为空，YaST 将会使用服务 URL 的一部分作为其名称。</p>\n"

#. popup message
#: src/modules/SourceDialogs.ycp:379
msgid "URL cannot be empty."
msgstr "URL 不能为空。"

#. text entry
#: src/modules/SourceDialogs.ycp:394
msgid "&URL"
msgstr "URL(&U)"

#. help text
#: src/modules/SourceDialogs.ycp:401
msgid ""
"<p><big><b>Repository URL</b></big><br>\n"
"Use <b>URL</b> to specify the URL of the repository.</p>"
msgstr ""
"<p><big><b>软件仓库 URL</b></big><br>\n"
"请使用 <b>URL</b> 指定软件仓库的 URL。<p>"

#. text entry
#: src/modules/SourceDialogs.ycp:412
msgid "&Server Name"
msgstr "服务器名(&S)"

#. text entry
#: src/modules/SourceDialogs.ycp:420 src/modules/SourceDialogs.ycp:1436
msgid "&Path to Directory or ISO Image"
msgstr "到目录或 ISO 映像的路径(&P)"

#. checkbox label
#: src/modules/SourceDialogs.ycp:427
msgid "&ISO Image"
msgstr "ISO 映像(&I)"

#. checkbox label
#: src/modules/SourceDialogs.ycp:429
msgid "N&FS v4 Protocol"
msgstr "NFS v4 协议(&F)"

#: src/modules/SourceDialogs.ycp:432
msgid "URL of the Repository"
msgstr "软件仓库的 URL"

#: src/modules/SourceDialogs.ycp:598 src/modules/SourceDialogs.ycp:1676
msgid "Edit Parts of the URL"
msgstr "编辑部分 URL"

#: src/modules/SourceDialogs.ycp:600 src/modules/SourceDialogs.ycp:1678
msgid "Edit Complete URL"
msgstr "编辑完整的 URL"

#. help text
#: src/modules/SourceDialogs.ycp:609
msgid ""
"<p><big><b>NFS Server</b></big><br>\n"
"Use <b>Server Name</b> and <b>Path to Directory or ISO Image</b>\n"
"to specify the NFS server host name and path on the server.</p>"
msgstr ""
"<p><big><b>NFS 服务器</b></big><br>\n"
"用<b>服务器名称</b>和<b>目录或 ISO 映象的路径</b>\n"
"指定 NFS 服务器主机名称和服务器上的路径。<p>"

#. radio button
#: src/modules/SourceDialogs.ycp:658
msgid "&CD-ROM"
msgstr "CD-ROM(&C)"

#. radio button
#: src/modules/SourceDialogs.ycp:660
msgid "&DVD-ROM"
msgstr "DVD-ROM(&D)"

#: src/modules/SourceDialogs.ycp:664
msgid ""
"<p><big><b>CD or DVD Media</b></big><br>\n"
"Set <b>CD-ROM</b> or <b>DVD-ROM</b> to specify the type of media.</p>"
msgstr ""
"<p><big><b>CD 或 DVD 介质</b></big><br>\n"
"设置 <b>CD-ROM</b> 或 <b>DVD-ROM</b> 指定介质类型。</p>"

#. dialog caption
